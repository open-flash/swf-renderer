    "object-inspect": "^1.6.0",
    "rxjs": "^6.5.3",
    "semantic-types": "^0.1.1",
    "swf-tree": "^0.8.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/gulp": "^4.0.6",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.7",
    "@types/pixelmatch": "^5.0.0",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "kryo": "^0.8.1",
    "minimist": "^1.2.3",
    "pixelmatch": "^5.1.0",
    "pre-commit": "^1.2.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "turbo-gulp": "^0.20.1",
    "typescript": "^3.6.3"
